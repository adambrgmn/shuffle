  "dependencies": {
    "@fransvilhelm/hooks": "^1.1.0",
    "@reach/menu-button": "^0.7.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "lodash-es": "^4.17.15",
    "nanoid": "^2.1.9",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
