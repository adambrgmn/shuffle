    "react-scripts": "3.3.0",
    "styled-components": "^4.4.1",
    "typescript": "~3.7.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.3",
    "@types/nanoid": "^2.1.0",
    "@types/reach__menu-button": "^0.1.0",
    "@types/react-router-dom": "^5.1.3",
